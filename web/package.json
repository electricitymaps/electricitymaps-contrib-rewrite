{
  "name": "electricitymap-web",
  "version": "1.1.0-beta.54",
  "description": "A real-time visualisation of the CO2 emissions of electricity consumption",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=14.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/electricityMaps/electricitymaps-contrib.git"
  },
  "scripts": {
    "build": "pnpm update-version && vite build",
    "update-version": "echo '{\"version\": \"'$npm_package_version'\"}' > public/client-version.json",
    "update-world": "ts-node ./geo/update-world.ts",
    "generate-index-files": "ts-node ./scripts/generate-index-files.ts",
    "generate-zones-config": "ts-node ./scripts/generate-zones-config.ts",
    "predevelop": "pnpm run generate-zones-config",
    "prebuild": "pnpm run generate-zones-config",
    "postinstall": "pnpm run generate-zones-config",
    "cy:components": "cypress open --components -b electron",
    "cy:e2e": "cypress open --e2e -b electron",
    "dev": "vite --open",
    "prepare": "cd .. && husky install web/.husky",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "test": "vitest",
    "test:ci": "vitest run",
    "format": "prettier -uw --cache --ignore-path ../.prettierignore .",
    "run-tsc": "tsc",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx .",
    "lint": "run-p run-tsc run-eslint",
    "validate": "run-p lint test:ci test:e2e:headless",
    "deploy": "pnpm version prerelease --preid=beta && pnpm build && pnpm run generate-index-files && sh scripts/deploy.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
